<html>
<head>
<title>Supremes</title>
<link rel="stylesheet" href="http://api.ryzom.com/data/css/ryzom_ui.css" />
<link rel="stylesheet" href="/css/index.css" />
</head>
<body>
<div id="main">
<div class="ryzom-ui ryzom-ui-header">
  <div class="ryzom-ui-tl">
    <div class="ryzom-ui-tr">
      <div class="ryzom-ui-t">
        Supremes
      </div>
    </div>
  </div>

  <div class="ryzom-ui-l">
    <div class="ryzom-ui-r">
      <div class="ryzom-ui-m">
        <div class="ryzom-ui-body">
          <table>
            <tbody>
              <tr>
                <td class="nav">
                <% Object.keys(zones).forEach(z => { %>
                  <a href="?zone=<%= z %>"><%= zones[z] %></a>
                <% }) %>
                </td>
                <td rowspan="2">
                  <ol>
                  <li><span class="hour">Time</span> Humidity</li>
                  <% weather.forEach(w => { %>
                    <li class="<%= w.weatherClass %>"><span class="hour"><%= w.ryzomTime.prettyHour() %></span> <%= w.humidity %></li>
                  <% }) %>
                  </ol>
                </td>
                <td rowspan="2">
                  <% if(zone) { %>
                    <h2><%= zones[zone] %> Materials - <%= seasons[weather[0].ryzomTime.season] %></h2>
                    <table>
                      <thead>
                        <tr>
                          <th>Material</th>
                          <th>Sup</th>
                        </tr>
                      </thead>
                      <tbody>
                        <% supremes.current.mats.forEach(mat => { %>
                          <tr>
                            <td class="<%= mat.type %>"><%= mat.name %> <%= mat.type %></td>
                            <td class="<%= weather[0].weatherClass %>">Now until <%= supremes.next.weather.ryzomTime.prettyHour() %></td>
                          </tr>
                        <% }) %>
                        <% supremes.next.mats.forEach(mat => { %>
                          <tr>
                            <td class="<%= mat.type %>"><%= mat.name %> <%= mat.type %></td>
                            <td class="<%= supremes.next.weather.weatherClass %>"><%= supremes.next.weather.ryzomTime.prettyHour() %></td>
                          </tr>
                        <% }) %>
                        <% supremes.afterThat.mats.forEach(mat => { %>
                          <tr>
                            <td class="<%= mat.type %>"><%= mat.name %> <%= mat.type %></td>
                            <td class="<%= supremes.afterThat.weather.weatherClass %>"><%= supremes.afterThat.weather.ryzomTime.prettyHour() %></td>
                          </tr>
                        <% }) %>
                        <% supremes.lastOne.mats.forEach(mat => { %>
                          <tr>
                            <td class="<%= mat.type %>"><%= mat.name %> <%= mat.type %></td>
                            <td class="<%= supremes.lastOne.weather.weatherClass %>"><%= supremes.lastOne.weather.ryzomTime.prettyHour() %></td>
                          </tr>
                        <% }) %>
                      </tbody>
                    </table>
                  <% } %>
                </td>
              </tr>
              <tr>
                <td>
                  <ol>
                    <li>weather</li>
                    <li class="worst">Worst (83% &uarr;)</li>
                    <li class="bad">Bad (50-83%)</li>
                    <li class="good">Good (16-50%)</li>
                    <li class="best">Best (16% &darr;)</li>
                  </ol>
                </td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>

  <div class="ryzom-ui-bl">
    <div class="ryzom-ui-br">
      <div class="ryzom-ui-b"></div>
    </div>
  </div>
</div>
</div>
</body>
</html>
